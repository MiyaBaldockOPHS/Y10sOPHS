import random
num = [1,5]
number = random.choice(num)
pick = int(input("pick a number between 1 and 5>>>"))
if pick == number:
    print("well done")
elif pick < number:
    print("too low")
    guess = int(input("guess again>>>"))
elif pick > number:
    print("too high")
    guess = int(input("guess again>>>"))

if guess == number:
    print("correct")
else:
    print("you lose")
    
